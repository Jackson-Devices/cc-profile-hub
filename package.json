{
  "name": "cc-profile-hub",
  "version": "1.0.0",
  "description": "account switcher for Claude Code CLI",
  "main": "dist/index.js",
  "bin": {
    "claude-wrapper": "dist/index.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "build": "tsc",
    "clean": "rm -rf dist coverage",
    "precommit": "npm run lint && npm run format:check && npm test",
    "prepush": "npm run precommit && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "http://local_proxy@127.0.0.1:58170/git/Jackson-Devices/cc-profile-hub"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "@types/proper-lockfile": "^4.1.4",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "commander": "^14.0.2",
    "js-yaml": "^4.1.1",
    "pino": "^10.1.0",
    "proper-lockfile": "^4.1.2",
    "zod": "^4.1.12"
  }
}
